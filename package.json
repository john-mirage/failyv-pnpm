{
  "name": "frontend-mentor-pnpm",
  "devDependencies": {
    "eslint": "^8.42.0",
    "eslint-config-custom": "workspace:*",
    "plop": "^3.1.2",
    "prettier": "^2.8.8",
    "syncpack": "^10.5.1"
  },
  "packageManager": "pnpm@7.15.0",
  "private": true,
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate:component": "plop component",
    "package:fix": "syncpack fix-mismatches",
    "package:format": "syncpack format",
    "package:list": "syncpack list-mismatches"
  }
}
